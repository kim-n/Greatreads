<%= @output %>
<h3>Books:</h3>
<table>
  <% @books.each do |book| %>
    <tr>
      <td>
        <a href="<%= book_url(book.isbn) %>">
          <img src="<%= book.pic %>" width="42">
        </a>
      </td>
      <td>
        <a href="<%= book_url(book.isbn) %>"><%= book.title %></a>
      </td>
    </tr>
  <% end %>
</table>

<h3>Clubs:</h3>
<table>
  <% @clubs.each do |club| %>
    <tr>
      <td><a href="<%= club_url(club) %>"><%= club.title %></a></td>
    </tr>
  <% end %>
</table>
