<%= button_to "Free ADMIN access", session_url(@book, {user: {email: "dummyAdmin", password: "whydidiletyouin"} }) %>
<%= button_to "Try App!", session_url(@book, {user: {email: "dummyRegular", password: "okayGO"} }) %>
