<%= @user.email %>
<%= @user.name %>

<br>
Wish List:
<table>
<% @wishes.each do |wish| %>
<tr>
  <td><a href="<%= book_url(wish.book.isbn) %>"><%= wish.book.title %></a></td>
 </tr>
<% end %>
</table>

<br>
Read List:
<table>
<% @reads.each do |read| %>
<tr>
  <td><%= read.taste == 1 ? "^ " : "V "%><a href="<%= book_url(read.book.isbn) %>"><%= read.book.title %></a></td>
 </tr>
<% end %>
</table>

<br>
Reviews:
<table>
<% @reviews.each do |review| %>
<tr>
  <th><a href="<%= book_url(review.bookisbn) %>"><%= review.bookname %></a></th>
  <td><%= review.title %></td>
  <td><%= review.body %></td>
 </tr>
<% end %>
</table>

<br>
Posts:
<table>
<% @posts.each do |post| %>
<tr>
  <th><%= post.bookname %></th>
  <td><a href="<%= club_url(post.club) %>"><%= post.title %></a></td>
  <td><%= post.body %></td>
 </tr>
<% end %>
</table>
